<div class="modal-content" data-region="feedback-content" data-id="{{ cmid }}">
    <div class="modal-header">

        <ul class="nav nav-tabs" id="myTab" role="tablist">
            {{# questions }}
                <li class="nav-item">
                    <a data-region="nav-item" class="nav-link" id="home-tab" data-toggle="tab" href="#q{{ cmid }}-{{ id }}" role="tab" aria-controls="q{{ cmid }}-{{ id }}" aria-selected="false">
                        {{ order }}
                    </a>
                </li>
            {{/ questions }}
        </ul>

        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>

    </div>
    <div class="modal-body">

        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="q{{ cmid }}-0" role="tabpanel" aria-labelledby="home-tab">
                <h3 class="modal-title">
                    {{# str }} want_start, mod_tipcoll {{/str}}
                </h3>
                <p class="modal-desc">
                    {{# str }} want_start_desc, mod_tipcoll {{/str}}
                </p>
                <div class="modal-actions">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">
                        {{# str }} cancel, mod_tipcoll {{/str}}
                    </button>
                    <a type="button" data-action="start" class="btn btn-primary" data-toggle="tab" href="#q{{ cmid }}-{{ firstid }}" role="tab" aria-controls="q{{ cmid }}-{{ id }}">
                        {{# str }} start, mod_tipcoll {{/str}}
                    </a>
                </div>
            </div>
            {{# questions }}
                <div class="tab-pane fade" id="q{{ cmid }}-{{ id }}" role="tabpanel" aria-labelledby="profile-tab">
                    {{> mod_tipcoll/question_component }}
                </div>
            {{/ questions }}
        </div>
    </div>

    <div class="modal-footer">
        <button class="btn btn-primary hidden" data-action="send_response" data-id="{{ cmid }}" disabled>
            {{# str }} send_feedback, mod_tipcoll {{/str}}
        </button>
    </div>

</div>
{{#js}}
    require(['mod_tipcoll/feedback'], function(feedback) {
        feedback.initFeedback('[data-region="feedback-content"]', {{ cmid }});
    });
{{/js}}
